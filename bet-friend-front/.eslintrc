{
  "env": {
    "browser": true,
    "jquery": true
  },
  "globals": {
    "angular": true,
    "amplify": true,
    "PNotify": true,
    "_" : true
  },
  "extends": "eslint:recommended",
  "rules": {
    "accessor-pairs": 2,
    "array-bracket-spacing": 0,
    "array-callback-return": 2,
    "arrow-body-style": [
      2,
      "always"
    ],
    "arrow-spacing": 2,
    "block-scoped-var": 2,
    "block-spacing": 2,
    "brace-style": [
      2,
      "1tbs"
    ],
    "callback-return": 2,
    "camelcase": 0,
    "comma-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "comma-style": [
      2,
      "last"
    ],
    "complexity": [
      1,
      {
        "max": 15
      }
    ],
    "computed-property-spacing": [
      2,
      "never"
    ],
    "consistent-return": 2,
    "consistent-this": 0,
    "curly": 2,
    "default-case": 2,
    "dot-location": [
      2,
      "property"
    ],
    "dot-notation": 2,
    "eol-last": 0,
    "eqeqeq": 2,
    "func-names": 0,
    "func-style": 0,
    "generator-star-spacing": 2,
    "global-require": 2,
    "guard-for-in": 0,
    "handle-callback-err": 2,
    "id-blacklist": 2,
    "id-length": 0,
    "id-match": 2,
    "indent": 0,
    "init-declarations": [
      2,
      "always"
    ],
    "jsx-quotes": 2,
    "key-spacing": [
      2,
      {
        "beforeColon": true,
        "afterColon": true
      }
    ],
    "keyword-spacing": 0,
    "linebreak-style": 0,
    "lines-around-comment": [
      2,
      {
        "beforeBlockComment": true
      }
    ],
    "max-depth": 0,
    "max-len": 0,
    "max-nested-callbacks": 2,
    "max-params": 0,
    "max-statements": 0,
    "max-statements-per-line": 2,
    "new-parens": 2,
    "newline-after-var": 0,
    "newline-before-return": 0,
    "newline-per-chained-call": 0,
    "no-alert": 2,
    "no-array-constructor": 2,
    "no-bitwise": 2,
    "no-caller": 2,
    "no-catch-shadow": 2,
    "no-confusing-arrow": 2,
    "no-continue": 2,
    "no-div-regex": 2,
    "no-duplicate-imports": 2,
    "no-else-return": 0,
    "no-empty-function": 0,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-label": 2,
    "no-extra-parens": [
      2,
      "all",
      {
        "conditionalAssign": false,
        "nestedBinaryExpressions": false
      }
    ],
    "no-floating-decimal": 2,
    "no-implicit-globals": 2,
    "no-implied-eval": 2,
    "no-inline-comments": 0,
    "no-inner-declarations": [
      2,
      "functions"
    ],
    "no-invalid-this": 0,
    "no-iterator": 2,
    "no-label-var": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-lonely-if": 0,
    "no-loop-func": 2,
    "no-magic-numbers": 0,
    "no-mixed-requires": 2,
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-multiple-empty-lines": 2,
    "no-native-reassign": 2,
    "no-negated-condition": 0,
    "no-nested-ternary": 2,
    "no-new": 2,
    "no-new-func": 2,
    "no-new-object": 2,
    "no-new-require": 2,
    "no-new-wrappers": 2,
    "no-octal-escape": 2,
    "no-param-reassign": [
      0,
      {
        "props": false
      }
    ],
    "no-path-concat": 2,
    "no-plusplus": [
      2,
      {
        "allowForLoopAfterthoughts": true
      }
    ],
    "no-process-env": 2,
    "no-process-exit": 2,
    "no-proto": 2,
    "no-restricted-globals": 2,
    "no-restricted-imports": 2,
    "no-restricted-modules": 2,
    "no-restricted-syntax": 2,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-shadow": 2,
    "no-shadow-restricted-names": 0,
    "no-spaced-func": 0,
    "no-sync": 2,
    "no-ternary": 0,
    "no-throw-literal": 2,
    "no-trailing-spaces": 0,
    "no-undef-init": 2,
    "no-undefined": 0,
    "no-underscore-dangle": 0,
    "no-unmodified-loop-condition": 2,
    "no-unneeded-ternary": [
      2,
      {
        "defaultAssignment": true
      }
    ],
    "no-unused-expressions": 2,
    "no-unused-vars": [
      2,
      {
        "vars": "all",
        "args": "none"
      }
    ],
    "no-use-before-define": 0,
    "no-useless-call": 2,
    "no-useless-concat": 2,
    "no-useless-constructor": 2,
    "no-useless-escape": 0,
    "no-var": 0,
    "no-void": 2,
    "no-warning-comments": 0,
    "no-whitespace-before-property": 0,
    "no-with": 2,
    "object-curly-spacing": 0,
    "object-shorthand": 0,
    "one-var": 0,
    "one-var-declaration-per-line": 0,
    "operator-assignment": 2,
    "operator-linebreak": 2,
    "padded-blocks": 0,
    "prefer-arrow-callback": 0,
    "prefer-const": 2,
    "prefer-reflect": 0,
    "prefer-rest-params": 0,
    "prefer-spread": 2,
    "prefer-template": 0,
    "quote-props": 0,
    "quotes": [
      2,
      "single",
      {
        "avoidEscape": true,
        "allowTemplateLiterals": true
      }
    ],
    "radix": [
      2,
      "always"
    ],
    "require-jsdoc": 2,
    "require-yield": 2,
    "semi": 2,
    "semi-spacing": 0,
    "sort-imports": 2,
    "sort-vars": 0,
    "space-before-blocks": 0,
    "space-before-function-paren": [
      2,
      "never"
    ],
    "space-in-parens": 0,
    "space-infix-ops": 0,
    "space-unary-ops": 2,
    "spaced-comment": 0,
    "strict": 2,
    "template-curly-spacing": 2,
    "valid-jsdoc": [
      2,
      {
        "prefer": {
          "return": "returns"
        },
        "preferType": {
          "String": "String",
          "object": "Object",
          "boolean": "Boolean",
          "controller": "Controller",
          "model": "Model",
          "service": "Service",
          "manager": "Manager"
        }
      }
    ],
    "vars-on-top": 0,
    "wrap-regex": 2,
    "yield-star-spacing": 2,
    "yoda": [
      2,
      "never"
    ]
  }
}